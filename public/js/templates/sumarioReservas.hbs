    <div class="summaryDiv panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Resumo de Aprovações</h3>
      </div>

      <div class="row">
        <div class="col-md-6 pdRightXs">
          <div class="text-center summarySeparator">
            <h5 class="bold h5margin">Geral</h5>
          </div>

          <div class="container-fluid">
            <div class="row text-center">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>Total</th>
                    <th>Quantidade</th>
                  </tr>
                </thead>
                <tbody>
                  <tr class="active">
                    <tr class="active">
                      <td>Mergulhadores</td>
                      <td>{{divers}}</td>
                    </tr>
                  </tr>
                  <tr class="active">
                    <tr class="active">
                      <td>Tanques</td>
                      <td>{{tanks}}</td>
                    </tr>
                  </tr>
                  <tr class="active">
                    <tr class="active">
                      <td>Quartos</td>
                      <td>{{innRooms}}</td>
                    </tr>
                  </tr>
                  <tr class="active">
                    <tr class="active">
                      <td>Equipamentos</td>
                      <td>{{gears}}</td>
                    </tr>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>

        <div class="col-md-6 pdLeftXs">
          <div class="text-center bold summarySeparator">
            <h5 class="bold h5margin">Tanques e Gases</h5>
          </div>

          <div class="container-fluid">
            <div class=" text-center">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>Tanque - Gas</th>
                    <th>Quantidade</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each tankInfo}}
                    <tr class="active">
                      <tr class="active">
                        <td>{{this.desc}}</td>
                        <td>{{this.qty}}</td>
                      </tr>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>

      </div>

      <div class="row">

        <div class="col-md-6 pdRightXs">
          <div class="text-center summarySeparator">
            <h5 class="bold h5margin">Equipamentos</h5>
          </div>
          <div class="container-fluid">
            <div class="text-center">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>Equipamento</th>
                    <th>Quantidade</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each gearInfo}}
                    <tr class="active">
                      <tr class="active">
                        <td>{{this.desc}}</td>
                        <td>{{this.qty}}</td>
                      </tr>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>

        <div class="col-md-6 pdLeftXs">
          <div class="text-center bold summarySeparator">
            <h5 class="bold h5margin">Pousada</h5>
          </div>

          <div class="container-fluid">
            <div class="text-center">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>Acomodação</th>
                    <th>Quantidade</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each innInfo}}
                    <tr class="active">
                      <tr class="active">
                        <td>{{this.desc}}</td>
                        <td>{{this.qty}}</td>
                      </tr>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>

      </div>

    </div>