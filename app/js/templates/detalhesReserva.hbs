<hr>
          <h4 class="modal-title b">Cliente:</h4>
          <div class="container-fluid container-client">
            <div class="row">
              <div class="col-md-2 bold">CPF/CNPJ:</div>
              {{#if client.cpf}}
                <div class="col-md-4">{{formatCpfCnpj client.cpf}}</div>
              {{else}}
                <div class="col-md-4">{{formatCpfCnpj client.cnpj}}</div>
              {{/if}}
              <div class="col-md-2 bold">CEP: </div>
              <div class="col-md-4">{{client.cep}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold">Nome:</div>
              <div class="col-md-4">{{client.name}}</div>
              <div class="col-md-2 bold">Endereço: </div>
              <div class="col-md-4">{{client.address}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold">Tel. Fixo:</div>
              <div class="col-md-4">{{formatCelTel client.tel}}</div>
              <div class="col-md-2 bold">Número:</div>
              <div class="col-md-4">{{client.number}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold">Celular:</div>
              <div class="col-md-4">{{formatCelTel client.cel}}</div>
              <div class="col-md-2 bold">Complemento: </div>
              <div class="col-md-4">{{client.comp}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold">Email:</div>
              <div class="col-md-4">{{client.email}}</div>
              <div class="col-md-2 bold">Bairro: </div>
              <div class="col-md-4">{{client.region}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold">Nível:</div>
              <div class="col-md-4">{{client.diverType.desc}}</div>
              <div class="col-md-2 bold">Cidade:</div>
              <div class="col-md-4">{{client.city}}</div>
            </div>
            <div class="row">
              <div class="col-md-2 bold"></div>
              <div class="col-md-4"></div>
              <div class="col-md-2 bold">UF: </div>
              <div class="col-md-4">{{client.uf}}</div>
            </div>
          </div>

          <hr>
          {{#if reservationComments}}
            <h4 class="modal-title b">Observações da reserva:</h4>
            <div class="container-fluid container-inn">
              <div class="row">
                <div class="col-md-12">{{reservationComments}}</div>
              </div>
            </div>

            <hr>
          {{/if}}
          

          <h4 class="modal-title b">Tanques:</h4>
          <div class="container-fluid container-tanks">
            <div class="row text-center">
              <table class="table table-striped table-hover ">
                <thead>
                  <tr>
                    <th>Tipo</th>
                    <th>Gás</th>
                    <th>Início Uso</th>
                    <th>Fim Uso</th>
                  </tr>
                </thead>
                <tbody>
                {{#each tankInfo}}
                  <tr class="active">
                    <td>{{this.tankType.type}}</td>
                    <td>{{this.gasType.type}}</td>
                    <td>{{formatDate this.dateIni}}</td>
                    <td>{{formatDate this.dateIni}}</td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
            </div>
          </div>

          <hr>

          {{#if gearNeeded}}
            <h4 class="modal-title b">Equipamentos:</h4>
              <div class="container-fluid container-inn">
                <div class="row text-center">
                  <table class="table table-striped table-hover ">
                    <thead>
                      <tr>
                        <th>Equipamento</th>
                        <th>Quantidade</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#each gearInfo}}
                        <tr class="active">
                          <tr class="active">
                            <td>{{this.desc}}</td>
                            <td>{{this.quantity}}</td>
                          </tr>
                        </tr>
                      {{/each}}
                    </tbody>
                  </table>
                </div>
              </div>

              <hr>
          {{/if}} 

          {{#if innNeeded}}
            <h4 class="modal-title b">Pousada:</h4>
            <div class="container-fluid container-inn">
              <div class="row">
                <div class="col-md-2 bold">Data de entrada:</div>
                <div class="col-md-10">12/12/2012</div>
              </div>
              <div class="row">
                <div class="col-md-2 bold">Data de saída:</div>
                <div class="col-md-10">12/12/2012</div>
              </div>
              <div class="row">
                <div class="col-md-2 bold">Tipo Apto.:</div>
                <div class="col-md-10">Duplo casal</div>
              </div>
              <div class="row">
                <div class="col-md-2 bold">Observações:</div>
                <div class="col-md-10">nada a observar</div>
              </div>
            </div>

            <div class="container-fluid container-inn">
              <div class="row text-center">
                <table class="table table-striped table-hover ">
                  <thead>
                    <tr>
                      <th>Hóspede</th>
                      <th>Tipo</th>
                      <th>Mergulhador</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr class="active">
                      <td>Felipe Roris Surerus</td>
                      <td>Responsável</td>
                      <td>Sim</td>
                    </tr>
                    <tr class="active">
                      <td>Bebel Nascimento</td>
                      <td>Acompanhante</td>
                      <td>Não</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          {{/if}}